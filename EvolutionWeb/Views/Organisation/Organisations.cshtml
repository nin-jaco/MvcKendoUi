@{
    ViewBag.ScreenDescription = "Edit Organisation";

    var OrganisationID = ViewBag.OrganisationID;

    var blnAdd = false;
    EvolutionRepository.Models.Organisation organisation = new EvolutionRepository.Models.Organisation();
    if (OrganisationID > 0)
    {
        //Editing;
        //organisation = EvolutionBusinessLogic.Organisations.OrganisationLogic.GetOrganisation(OrganisationID);
    }
    else
    {
        blnAdd = true;
        //Creating;
    }

    var chkRadioButton = "";

    if (string.IsNullOrEmpty(organisation.Longitude.ToString()))
    {
        if (string.IsNullOrEmpty(organisation.Longitude.ToString()))
        {
            chkRadioButton = "Decimal";
        }
        else
        {
            chkRadioButton = "DMS";
        }

    }
    else
    {
        chkRadioButton = "Decimal";
    }

    var chkVATRadioButton = "";

    if (string.IsNullOrEmpty(organisation.Longitude.ToString()))
    {
        chkVATRadioButton = "No";
    }
    else
    {
        chkVATRadioButton = "Yes";
    }

    var isRouteOperator = "";

    if (!string.IsNullOrEmpty(organisation.Longitude.ToString()))
    {
        if (organisation.Longitude > 0)
        {
            isRouteOperator = "true";
        }
        else
        {
            isRouteOperator = "false";
        }

    }
    else
    {
        isRouteOperator = "false";
    }

    string RouteOperator = organisation.ParentID.ToString();
    string ServiceProvider = organisation.ParentID.ToString();
    string Manufacturer = organisation.ParentID.ToString();
    string Supplier = organisation.ParentID.ToString();
}



<section class="four columns">
    <div class="panel">
        <h3>organisation menu</h3>
        <p>Please select one of the options:</p>
        @Html.Partial(Url.Content("~/Views/Shared/SideMenu/_SideMenuOrganisation.cshtml"))

    </div>
</section>

<section class="eight columns">
    <input type="hidden" id="RouteOperator" name="RouteOperator" value="@RouteOperator" />
    <input type="hidden" id="ServiceProvider" name="ServiceProvider" value="@ServiceProvider" />
    <input type="hidden" id="Manufacturer" name="Manufacturer" value="@Manufacturer" />
    <input type="hidden" id="Supplier" name="Supplier" value="@Supplier" />
    <input type="hidden" id="chkradiobutton" name="chkradiobutton" value="@chkRadioButton" >
    <input type="hidden" id="chkvatradiobutton" name="chkvatradiobutton" value="@chkVATRadioButton" >
    <input type="hidden" id="isrouteoperator" name="isrouteoperator" value="@isRouteOperator" >

    <article class="blog_post">
        <div class="twelve columns">
            <h3>Organisation Details</h3>
            <input value="@organisation.OrganisationID" type="hidden" id="selectedOrgID" />
            <div class="row">
                <div class="six columns">
                    <label>Organisation Name</label>
                    <input id="txtOrgName" name="txtOrgName" value="@organisation.OrganisationName" class="depth" style="width:250px" />
                </div>
                <div class="six columns">
                    <label>Parent Organisation</label>
                    <input id="ddlParentID" name="ddlParentID" value="@organisation.ParentID" style="width:250px" />
                </div>
            </div>
            <div class="row">
                <div class="six columns">
                    <label>Jurisdiction</label>
                    <input id="ddlJurisdictionID" name="ddlJurisdictionID" value="@organisation.JurisdictionID" style="width:250px"/>
                </div>
                <div class="six columns">
                    <label>Company Registration No.</label>
                    <input id="txtRegistrationNo" name="txtRegistrationNo" value="@organisation.CompanyNumber" style="width:250px" class="depth"/>
                </div>
            </div>
            <div class="row">
                <div class="six columns">
                    <label>District</label>
                    <input id="ddlDistrict" name="ddlDistrict" value="@organisation.CompanyNumber" style="width:250px"/>
                </div>
                <div class="six columns">
                    <label>Maximum Machines</label>
                    <input id="numMaximumMachines" name="numMaximumMachines" value="@organisation.MaximumMachines" style="width:250px"/>
                </div>
            </div>
            <div class="row">
                <div class="six columns">
                    <label>Type</label>
                    <input id="ddlOrgType" name="ddlOrgType" value="@organisation.OrganisationTypeID" style="width:250px"/>
                </div>
                <div class="six columns">
                    <label>Assigned Area</label>
                    <input id="ddlAssignedArea" name="ddlAssignedArea" value="@organisation.OrganisationTypeID" style="width:250px"/>
                </div>
            </div>
            <div class="row">
                <div class="six columns">
                    <label>Sub Type</label>
                    <input id="ddlOrgSubType" name="ddlOrgSubType" value="@organisation.OrganisationSubTypeID" style="width:250px"/>
                </div>
                <div class="six columns">
                    <label>Site Status</label>
                    <input id="ddlOrganisationStatusID" name="ddlOrganisationStatusID" value="@organisation.OrganisationStatusID" style="width:250px"/>
                </div>
            </div>
            <div class="row">
                <div class="six columns">
                    <label>VAT Vendor</label>
                    <input class="float-left" type="radio" name="radio-vatchoice" id="radio-vatchoice-1" value="choice-1" />Yes &nbsp; &nbsp;
                    <input class="float-left" type="radio" name="radio-vatchoice" id="radio-vatchoice-2" value="choice-2" />No
                </div>
                <div class="six columns">
                    <label></label>
                </div>
            </div>
            <div class="row">
                <div class="six columns">
                    <label>Registration No.</label>
                    <input id="txtVATVendor" name="txtVATVendor" value="@organisation.TaxNumber" style="width:250px" class="depth"/>
                </div>
                <div class="six columns">
                    <label>Tax number</label>
                    <input id="txtTaxNumber" name="txtTaxNumber" value="@organisation.TaxNumber" placeholder="000000" style="width:250px" class="depth"/>
                </div>
            </div>
        </div>
        

    </article>

    <article class="blog_post">
        <div class="twelve columns">
            <h3>Contact Details</h3>
