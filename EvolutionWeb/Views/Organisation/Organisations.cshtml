                    <label>Longitude (S)</label>
                    <input id="txtLongD" name="txtLongD" value="@organisation.Longitude" style="width:48px" class="depth"/>
                    <input id="txtLongM" name="txtLongM" value="@organisation.Longitude" style="width:48px" class="depth"/>
                    <input id="txtLongS" name="txtLongS" value="@organisation.Longitude" style="width:48px" class="depth"/>
                </div>
                <div class="six columns">
                    <label>Latitude (E)</label>
                    <input id="txtLatD" name="txtLatD" value="@organisation.Latitude" style="width:48px" class="depth"/>
                    <input id="txtLatM" name="txtLatM" value="@organisation.Latitude" style="width:48px" class="depth"/>
                    <input id="txtLatS" name="txtLatS" value="@organisation.Latitude" style="width:48px" class="depth"/>
                </div>
