    }
    else {
        $("#Supplier").val("off");
        $('input[name=checkSupplier]').prop('checked', false);
    }
        
    $('#checkRouteOperator').change(function () {
        if ($('#checkRouteOperator').is(':checked')) {
            $("#RouteOperator").val("on");
        }
        else {
            $("#RouteOperator").val("off");
        }
    })

    $('#checkServiceProvider').change(function () {
        if ($('#checkServiceProvider').is(':checked')) {
            $("#ServiceProvider").val("on");
        }
        else {
            $("#ServiceProvider").val("off");
        }
    })

