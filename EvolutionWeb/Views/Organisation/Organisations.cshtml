    (function LoadOrgSubTypes() {
        var apiUrl = "@VirtualPathUtility.ToAbsolute("~/api/OrganisationAPI/GetOrgSubtypes/1")";

        $.get(apiUrl, "", function (data, textStatus, jqXHR) {
