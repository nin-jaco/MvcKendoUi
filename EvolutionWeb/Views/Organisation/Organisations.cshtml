    }); //document.ready

    if ($("#RouteOperator").val() == "on") {
        $("#RouteOperator").val("on");
        $('input[name=checkRouteOperator]').prop('checked', true);
    }
    else {
        $("#RouteOperator").val("off");
        $('input[name=checkRouteOperator]').prop('checked', false);
    }

    if ($("#ServiceProvider").val() == "on") {
        $("#ServiceProvider").val("on");
        $('input[name=checkServiceProvider]').prop('checked', true);
    }
    else {
        $("#ServiceProvider").val("off");
        $('input[name=checkServiceProvider]').prop('checked', false);
    }

